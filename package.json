{
  "name": "example-wc",
  "version": "0.0.0-0",
  "description": "Web Component Workspace",
  "repository": {
    "type": "git",
    "url": "https://github.com/pouncyisdead/sb1-odb14eir.git"
  },
  "license": "MIT or CC-BY-SA-4.0",
  "private": true,
  "scripts": {
    "nx-repair": "npx nx repair",
    "nx-reset": "npx nx reset",
    "nx-fix": "run-s nx-repair nx-reset",
    "build": "npx nx run-many --target=build --exclude=example-wc",
    "test": "npx nx run-many --target=test --exclude=example-wc",
    "lint": "nx exec -- eslint --max-warnings 0",
    "lint:nx": "npx nx run-many --target=lint --exclude=example-wc",
    "precommit": "nx run-many --targets=lint,test --exclude=example-wc",
    "serve:components": "npx nx serve components",
    "build:components": "npx nx build components",
    "test:components": "npx nx test components",
    "lint:components": "npx nx lint components",
    "build-storybook:components": "npx nx build-storybook components",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "validate": "npm-run-all --parallel type-check lint test"
  },
  "nx": {
    "extends": "@nx/workspace/presets/npm.json"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@angular-devkit/core": "~19.0.0",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.17.0",
    "@nx/angular": "^20.3.0",
    "@nx/devkit": "^20.3.0",
    "@nx/eslint": "^20.3.0",
    "@nx/eslint-plugin": "^20.3.0",
    "@nx/jest": "^20.3.0",
    "@nx/js": "^20.3.0",
    "@nx/module-federation": "^20.3.0",
    "@nx/playwright": "^20.3.0",
    "@nx/plugin": "^20.3.0",
    "@nx/react": "^20.3.0",
    "@nx/storybook": "^20.3.0",
    "@nx/vite": "^20.3.0",
    "@nx/web": "^20.3.0",
    "@nx/webpack": "^20.3.0",
    "@nx/workspace": "^20.3.0",
    "@nxext/stencil": "20.0.6",
    "@nxkit/style-dictionary": "^6.0.0",
    "@stencil/core": "^4.23.0",
    "@stencil/sass": "^3.0.12",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@storybook/web-components": "^8.4.7",
    "@storybook/web-components-vite": "^8.4.7",
    "@swc-node/register": "^1.10.1",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.1",
    "@swc/helpers": "~0.5.15",
    "@types/jest": "~29.5.14",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/ui": "^1.3.1",
    "chokidar": "3.6.0",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-storybook": "^0.11.1",
    "glob": "^9.3.5",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lit": "^3.2.1",
    "npm-run-all": "^4.1.5",
    "nx": "^20.3.0",
    "prettier": "^3.0.0",
    "puppeteer": "^23.10.4",
    "rimraf": "^4.4.1",
    "sass": "^1.83.1",
    "storybook": "^8.4.7",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.19.0",
    "vite": "^5.4.11",
    "vitest": "^1.3.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "overrides": {
    "chokidar": "3.6.0",
    "glob": "$glob",
    "rimraf": "$rimraf"
  }
}
